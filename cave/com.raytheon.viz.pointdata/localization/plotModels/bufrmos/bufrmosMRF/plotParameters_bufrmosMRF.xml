<plotParameterDefinitions>
   <plugin>bufrmos/bufrmosMRF</plugin>

   <plotParameterDefinition displayName="stationId" displayType="TEXT" param="stationId" sampleValue="0" svgClass="text" />

   <plotParameterDefinition displayName="precipMix" displayType="TEXT" format="%3.0f" param="precipMix" sampleValue="1" svgClass="text" />

   <plotParameterDefinition displayName="Marker" displayType="MARKER" param="Marker" sampleValue=" " svgClass="marker" />

   <plotParameterDefinition displayName="precipSnow" displayType="TEXT" format="%3.0f" param="precipSnow" sampleValue="1" svgClass="text" />

   <plotParameterDefinition displayName="precipRain" displayType="TEXT" format="%3.0f" param="precipRain" sampleValue="1" svgClass="text" />

   <plotParameterDefinition displayName="maxTempDay (°F)" displayType="TEXT" format="%3.0f" param="maxTempDay" sampleValue="1" svgClass="text" unit="°F" />

   <plotParameterDefinition displayName="minTempNight (°F)" displayType="TEXT" format="%3.0f" param="minTempNight" sampleValue="1" svgClass="text" unit="°F" />

   <plotParameterDefinition displayName="stdTemperature (°F)" displayType="TEXT" format="%3.0f" param="stdTemperature" sampleValue="1" svgClass="text" unit="°F" />

   <plotParameterDefinition displayName="stdDewpoint (°F)" displayType="TEXT" format="%3.0f" param="stdDewpoint" sampleValue="1" svgClass="text" unit="°F" />

   <plotParameterDefinition displayName="presWeather" displayType="TABLE" lookupTable="wx_symbol_trans.txt" param="presWeather" recursiveLookup="true" sampleValue="0" svgClass="weather" />

   <plotParameterDefinition displayName="totalSkyCover" displayType="TABLE" lookupTable="bufrmos/mos_clouds_gfsex_s2s.txt" param="totalSkyCover" recursiveLookup="true" sampleValue="1" svgClass="weather" />

   <plotParameterDefinition displayName="POP12hr" displayType="TEXT" format="%3.0f" param="POP12hr" sampleValue="1" svgClass="text" />
</plotParameterDefinitions>
