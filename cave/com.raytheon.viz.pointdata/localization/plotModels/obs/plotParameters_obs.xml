<plotParameterDefinitions>
   <plugin>obs</plugin>

   <plotParameterDefinition displayName="rawMETAR" displayType="TEXT" param="rawMETAR" sampleValue="R" />

   <plotParameterDefinition displayName="windSpeed,windDir,windGust (kts)" displayType="BARB" param="windSpeed,windDir,windGust" sampleValue="6,7,8" unit="kts" />

   <plotParameterDefinition displayName="windBarb" displayType="BARB" param="windSpeed,windDir" sampleValue="16,17" unit="kts" />

   <plotParameterDefinition displayName="windDir" displayType="ARROWUV" param="windSpeed,windDir" sampleValue="16,17"  />

   <plotParameterDefinition displayName="windGust (kts)" displayType="TEXT" param="windGust" sampleValue="8" unit="kts" />

   <plotParameterDefinition displayName="pressChangeChar" displayType="TABLE" lookupTable="press_change_char_lookup.txt" param="pressChangeChar" sampleValue="5" svgClass="weather" />

   <plotParameterDefinition displayName="visibility (mi)" displayType="RANGE" format="%3.5f" lookupTable="fractions_lookup.txt" param="visibility" sampleValue="0" svgClass="text" unit="mi" />

   <plotParameterDefinition displayName="pressChange3Hour (Pa)" displayType="TEXT" format="%02.0f" param="pressChange3Hour" sampleValue="4.3" unit="Pa" />

   <plotParameterDefinition displayName="DpT (°F)" displayType="TEXT" format="%3.0f" param="DpT" sampleValue="43" unit="°F" />

   <plotParameterDefinition displayName="T (°F)" displayType="TEXT" format="%3.0f" param="T" sampleValue="76" unit="°F" />

   <plotParameterDefinition displayName="seaLevelPress (daPa)" displayType="TEXT" format="%5.0f" param="seaLevelPress" sampleValue="13" trim="2" unit="daPa" />

   <plotParameterDefinition displayName="skyCover" displayType="TABLE" functionTable="cloud_select.txt" lookupTable="cloud_chars.txt" param="skyCover" sampleValue="SCT" svgClass="special" />

   <plotParameterDefinition displayName="skyCover-0" index="0" displayType="TABLE" functionTable="cloud_select.txt" lookupTable="cloud_chars2.txt" param="skyCover" sampleValue="SCT" svgClass="special" />

   <plotParameterDefinition displayName="skyCover-1" index="1" displayType="TABLE" functionTable="cloud_select.txt" lookupTable="cloud_chars2.txt" param="skyCover" sampleValue="SCT" svgClass="special" />

   <plotParameterDefinition displayName="skyCover-2" index="2" displayType="TABLE" functionTable="cloud_select.txt" lookupTable="cloud_chars2.txt" param="skyCover" sampleValue="SCT" svgClass="special" />

   <plotParameterDefinition displayName="skyCover-3" index="3" displayType="TABLE" functionTable="cloud_select.txt" lookupTable="cloud_chars2.txt" param="skyCover" sampleValue="SCT" svgClass="special" />

   <plotParameterDefinition displayName="skyCover-4" index="4" displayType="TABLE" functionTable="cloud_select.txt" lookupTable="cloud_chars2.txt" param="skyCover" sampleValue="SCT" svgClass="special" />

   <plotParameterDefinition displayName="presWeather" displayType="TABLE" lookupTable="wx_symbol_trans.txt" param="presWeather" recursiveLookup="true" sampleValue="0" svgClass="weather" />

   <plotParameterDefinition displayName="pkwndSpeed (kts)" displayType="TEXT" format="PK%.0f" param="pkwndSpeed" sampleValue="17" unit="kts" />

   <plotParameterDefinition displayName="Marker" displayType="MARKER" param="Marker" sampleValue=" " svgClass="marker" />

