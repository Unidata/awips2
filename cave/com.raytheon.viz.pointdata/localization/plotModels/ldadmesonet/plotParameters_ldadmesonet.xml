<plotParameterDefinitions>
   <plugin>ldadmesonet</plugin>

   <plotParameterDefinition displayName="stationId,observationTime" displayType="TEXT" param="stationId,observationTime" sampleValue="0,1" svgClass="text" />
   
   <plotParameterDefinition displayName="temperature,dewpoint,relHumidity,wetBulbTemperature" displayType="TEXT" param="temperature,dewpoint,relHumidity,wetBulbTemperature" sampleValue="0,1,2,3" svgClass="text" />

   <plotParameterDefinition displayName="windDir,windSpeed,windGust" displayType="TEXT" param="windDir,windSpeed,windGust" sampleValue="0,1,2" svgClass="text" />

   <plotParameterDefinition displayName="pressure,stationPressure,altimeter,seaLevelPressure,pressChangeChar,pressChange3Hour" displayType="TEXT" param="pressure,stationPressure,altimeter,seaLevelPressure,pressChangeChar,pressChange3Hour" sampleValue="0,1,2,3,4,5" svgClass="text" />

   <plotParameterDefinition displayName="precipAccum,precipIntensity,precipType" displayType="TEXT" param="precipAccum,precipIntensity,precipType" sampleValue="0,1,2" svgClass="text" />

   <plotParameterDefinition displayName="totalCloudCover,skyCover,skyLayerBase,cloudBaseHeight,visibility,visibilityStatus,presWeather" displayType="TEXT" param="totalCloudCover,skyCover,skyLayerBase,cloudBaseHeight,visibility,visibilityStatus,presWeather" sampleValue="0,1,2,3,4,5,6" svgClass="text" />
   
   <plotParameterDefinition displayName="temperature (°F)" displayType="TEXT" format="%3.0f" param="temperature" sampleValue="75" unit="°F" />

   <plotParameterDefinition displayName="DpT (°F)" displayType="TEXT" format="%3.0f" param="DpT" sampleValue="59" unit="°F" />

   <plotParameterDefinition displayName="seaLevelPressure (daPa)" displayType="TEXT" format="%5.0f" param="seaLevelPressure" sampleValue="018" trim="2" unit="daPa" />

   <plotParameterDefinition displayName="windSpeed,windDir,windGust (kts)" displayType="BARB" param="windSpeed,windDir,windGust" sampleValue="0,1,2" unit="kts" />

   <plotParameterDefinition displayName="windBarb" displayType="BARB" param="windSpeed,windDir" sampleValue="16,17" unit="kts" />

   <plotParameterDefinition displayName="windDir" displayType="ARROWUV" param="windSpeed,windDir" sampleValue="16,17" unit="kts" />

   <plotParameterDefinition displayName="windGust (kts)" displayType="TEXT" param="windGust" sampleValue="8" unit="kts" />

   <plotParameterDefinition displayName="Marker" displayType="MARKER" param="Marker" sampleValue=" " svgClass="marker" />

   <plotParameterDefinition displayName="HIWC (°F)" displayType="TEXT" format="%3.0f" param="HIWC" sampleValue="75" unit="°F" />
</plotParameterDefinitions>
