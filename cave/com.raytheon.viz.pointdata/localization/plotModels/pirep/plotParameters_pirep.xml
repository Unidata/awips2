<plotParameterDefinitions>
   <plugin>pirep</plugin>

   <plotParameterDefinition displayName="obsText" displayType="TEXT" param="obsText" sampleValue="O" />
   
   <plotParameterDefinition displayName="iceType" displayType="TABLE" lookupTable="icing_type_trans.txt" param="iceType" sampleValue="0" svgClass="weather" />
   
   <plotParameterDefinition displayName="iceInten" displayType="TABLE" lookupTable="icing_intens_trans.txt" param="iceInten" sampleValue="0" svgClass="weather" />

   <plotParameterDefinition displayName="flightLevel (hft)" displayType="TEXT" format="%3.0f" param="flightLevel" sampleValue="75" unit="hft" />

   <plotParameterDefinition displayName="Marker" displayType="MARKER" param="Marker" sampleValue=" " svgClass="marker" />

   <plotParameterDefinition displayName="obsText,stationId,longitude,latitude,timeObs,flightLevel" displayType="TEXT" param="obsText,stationId,longitude,latitude,timeObs,flightLevel" sampleValue="0,1,2,3,4,5" />

   <plotParameterDefinition displayName="temperature,windDir,windSpeed,turbInten,turbFreq,turbType,iceInten,iceType" displayType="TEXT" param="temperature,windDir,windSpeed,turbInten,turbFreq,turbType,iceInten,iceType" sampleValue="0,1,2,3,4,5,6,7" />

   <plotParameterDefinition displayName="obsText,turbInten,iceInten" displayType="TEXT" param="obsText,turbInten,iceInten" sampleValue="0,1,2" svgClass="text" />

   <plotParameterDefinition displayName="temperature" displayType="TEXT" format="%3.0f" param="temperature" sampleValue="75" />

   <plotParameterDefinition displayName="windSpeed,windDir (kts)" displayType="BARB" param="windSpeed,windDir" sampleValue="0,1" unit="kts" />
   
   <plotParameterDefinition displayName="windDir" displayType="ARROWUV" param="windSpeed,windDir" sampleValue="0,1" unit="kts" />

   <plotParameterDefinition displayName="turbInten" displayType="TABLE" lookupTable="turb_intens_trans.txt" param="turbInten" sampleValue="0" svgClass="weather" />

   <plotParameterDefinition displayName="turbFreq" displayType="TABLE" lookupTable="turb_freq_trans.txt" param="turbFreq" sampleValue="0" svgClass="weather" />
</plotParameterDefinitions>
