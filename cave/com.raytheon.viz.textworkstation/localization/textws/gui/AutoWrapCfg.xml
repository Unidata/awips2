    <!--
        This is an incremental override file, indicating that the files at
        different localization levels will be combined. 
        See notes below for more information on how files are combined.
    -->
    
<autoWrapCfg>
    <!-- RangeStart/RangeEnd define the valid range for WrapCol
    
        if rangeStart or rangeEnd are present they will replace values from 
        a lower level file 
    -->
    <RangeStart>69</RangeStart>
    <RangeEnd>80</RangeEnd>
    
	<!-- if any buttons are present they will replace all buttons from a lower 
		level file. Off and Other are automatically generated and cannot be overridden.
	-->
	<WrapButtonCfg>
	    <!-- LabelName is optional, if not present "nn chars" will be used -->
	    <!-- 
		<LabelName>xxxxx</LabelName>
		 -->
		<WrapCol>69</WrapCol>
		<Selected>true</Selected>
	</WrapButtonCfg>

	<WrapButtonCfg>
		<WrapCol>72</WrapCol>
	</WrapButtonCfg>

	<WrapButtonCfg>
		<WrapCol>80</WrapCol>
	</WrapButtonCfg>

	<!-- NonWrapPils will be merged with those from lower level files -->
	<NonWrapPil>AFM</NonWrapPil>
	<NonWrapPil>PFM</NonWrapPil>
	<NonWrapPil>FWF</NonWrapPil>
	<NonWrapPil>SFT</NonWrapPil>
	<NonWrapPil>WCN</NonWrapPil>
	<NonWrapPil>FWS</NonWrapPil>
	<NonWrapPil>TCV</NonWrapPil>
	<NonWrapPil>HLS</NonWrapPil>
	
	<!-- WrapPil can be used to remove NonWrapPils from lower level files -->
	<!-- 
	<WrapPil>XXX</WrapPil>
	 -->
	
</autoWrapCfg>