<plotParameterDefinitions>
<plugin>modelsounding</plugin>

<plotParameterDefinition displayName="stationId" displayType="TEXT" param="stationId" sampleValue="0" svgClass="text" />

<plotParameterDefinition displayName="T (F)" displayType="TEXT" format="%2.0f" param="T" sampleValue="75" unit="F" />

<plotParameterDefinition displayName="DpT (F)" displayType="TEXT" format="%2.0f" param="DpT" sampleValue="75" unit="F" />

<plotParameterDefinition displayName="dP3hr (daPa)" displayType="TEXT" format="%02.0f" param="dP3hr" sampleValue="018" unit="daPa" />

<plotParameterDefinition displayName="msl-P (hPa)" displayType="TEXT" format="%5.0f" param="msl-P" sampleValue="75" trim="2" unit="hPa" />

<plotParameterDefinition displayName="CCP" displayType="RANGE" lookupTable="pct_cover_chars.txt" param="CCP" sampleValue="018" svgClass="special" />

<plotParameterDefinition displayName="wSp,WD (kts)" displayType="BARB" param="wSp,WD" sampleValue="0,1" unit="kts" />

<plotParameterDefinition displayName="WD" displayType="ARROWUV" param="wSp,WD" sampleValue="0,1" unit="kts" />

<plotParameterDefinition displayName="Marker" displayType="MARKER" param="Marker" sampleValue=" " svgClass="marker" />

<plotParameterDefinition displayName="totPrecip (kg/(m*m)*0.003937)" displayType="RANGE" format="%6.2f" lookupTable="prcp_formats.txt" param="totPrecip" sampleValue="0" svgClass="text" unit="kg/(m*m)*0.003937" />

<plotParameterDefinition displayName="lowCld" displayType="RANGE" lookupTable="pct_cover_chars.txt" param="lowCld" sampleValue="018" svgClass="special" />

<plotParameterDefinition displayName="midCld" displayType="RANGE" lookupTable="pct_cover_chars.txt" param="midCld" sampleValue="018" svgClass="special" />

<plotParameterDefinition displayName="hiCld" displayType="RANGE" lookupTable="pct_cover_chars.txt" param="hiCld" sampleValue="018" svgClass="special" />

<plotParameterDefinition displayName="prCloudHgtLow (m/0.032808399)" displayType="TEXT" format="%.0f" param="prCloudHgtLow" sampleValue="018" svgClass="text" unit="m/0.032808399" />

<plotParameterDefinition displayName="prCloudHgtMid (m/0.032808399)" displayType="TEXT" format="%.0f" param="prCloudHgtMid" sampleValue="018" svgClass="text" unit="m/0.032808399" />

<plotParameterDefinition displayName="prCloudHgtHi (m/0.032808399)" displayType="TEXT" format="%.0f" param="prCloudHgtHi" sampleValue="018" svgClass="text" unit="m/0.032808399" />

<plotParameterDefinition displayName="wx" displayType="TABLE" lookupTable="bufr_wx_symbol_trans.txt" param="wx" sampleValue="018" svgClass="weather" />

<plotParameterDefinition displayName="vsby (mi)" displayType="RANGE" lookupTable="fractions_lookup.txt" param="vsby" sampleValue="018" svgClass="text" unit="mi" />
</plotParameterDefinitions>