<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="com.raytheon.viz.bcd.openBCD"
            name="Open BCD">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.raytheon.viz.bcd.OpenBCDHandler"
            commandId="com.raytheon.viz.bcd.openBCD">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:import">
         <command
               commandId="com.raytheon.viz.bcd.openBCD"
               label="BCD File..."
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="com.raytheon.uf.viz.core.resource">
      <resource
            class="com.raytheon.viz.bcd.BCDResource"
            name="BCD Resource"
            renderingOrderId="MAP_OUTLINE">
         <fileType
               fileExtension="bcd"
               name="BCD File">
         </fileType>
      </resource>
   </extension>
</plugin>
