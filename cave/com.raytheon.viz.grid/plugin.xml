<?eclipse version="3.2"?>
<plugin>
   <extension-point id="com.raytheon.viz.grid.ext" name="GridExtension" schema="schema/com.raytheon.viz.grid.ext.exsd"/>
   <extension
         point="com.raytheon.uf.viz.core.resource">
      <resource
            class="com.raytheon.uf.viz.d2d.grid.RcmResource"
            name="RcmResource"
            recordClass="com.raytheon.uf.common.dataplugin.grid.GridRecord"
            renderingOrderId="IMAGE_COUNTRY"
            resourceType="PLAN_VIEW">
      </resource>
   </extension>
   <extension
         point="com.raytheon.uf.viz.core.units">
      <units
            class="com.raytheon.viz.grid.units.GridUnits"
            name="GridUnits">
      </units>
   </extension>
  
      <extension
         point="com.raytheon.uf.viz.productbrowser.dataDefinition">
         <dataDefinition
         	name="gridProductBrowserDataDefinition"
         	class="com.raytheon.viz.grid.GridProductBrowserDataDefinition" >
         </dataDefinition>
   </extension>
     <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="com.raytheon.uf.viz.productbrowser.DataTypePreferencePage"
            id="com.raytheon.viz.grid.productbrowser.productbrowserdatapreferencepage"
            name="Grid"
            category="com.raytheon.uf.viz.productbrowser.productbrowserpreferencespage"/>
   </extension>
     <extension
           point="org.eclipse.ui.menus">
        <menuContribution
              locationURI="menu:org.eclipse.ui.main.menu?after=radar">
           <menu
                 id="mrms"
                 label="MRMS">
              <visibleWhen>
                 <reference
                       definitionId="com.raytheon.uf.viz.d2d.ui.inD2DActionSet">
                 </reference>
              </visibleWhen>

              <separator name="MRMS_MENU_START" visible="false"/>
           </menu>
        </menuContribution>
     </extension>
   <extension
         point="com.raytheon.uf.viz.localization.perspective.localizationpath">
      <path
            application="EDEX"
            localizationType="COMMON_STATIC"
            name="Parameter"
            value="parameter"
            recursive="true" />
   </extension>
</plugin>
