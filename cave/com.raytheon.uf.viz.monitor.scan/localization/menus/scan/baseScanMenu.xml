<menuTemplate xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <contribute xsi:type="titleItem" titleText="------ SCAN: ${icao} ------"
        id="placeholderSCAN${icao}" />
    <contribute xsi:type="bundleItem"
        file="bundles/scan/stormCellsStormThreat.xml" menuText="Storm Cell/ Site Storm Threat"
        id="SCAN${icao}" commandId="com.raytheon.uf.viz.monitor.scan.openCELLTable">
        <substitute key="icao" value="${icao}" />
        <substitute key="fieldName" value="CWAT" />
        <substitute key="type" value="CELL" />
    </contribute>
    <contribute xsi:type="bundleItem"
        file="bundles/scan/stormDMDIconsTable.xml" menuText="DMD Icons And Table"
