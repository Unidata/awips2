        <substitute key="icao" value="${icao}" />
        <substitute key="type" value="DMD" />
    </contribute>
    <contribute xsi:type="subMenu" menuText="QPF from ${icao}"
        id="QPFfrom${icao}">
        <contribute xsi:type="separator" id="${icao}belowQPF" />
