			menuText="Storm Cell/ Site Storm Threat" id="SCAN${icao}" commandId="com.raytheon.uf.viz.monitor.scan.openCELLTable">
			<dataURI>/scan/%/${icao}/CELL/%</dataURI>
			<substitute key="icao" value="${icao}" />
