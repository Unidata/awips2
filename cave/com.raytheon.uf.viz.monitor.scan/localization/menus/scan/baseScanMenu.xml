            <substitute key="fieldName" value="P50" />
        </contribute>
        <contribute xsi:type="bundleItem" file="bundles/scan/QPF.xml"
            menuText="QPF .75" id="${icao}QPF .75">
            <substitute key="icao" value="${icao}" />
            <substitute key="fieldName" value="P75" />
        </contribute>
        <contribute xsi:type="bundleItem" file="bundles/scan/QPF.xml"
