        <substitute key="fieldName" value="CWAT" />
        <substitute key="colormap" value="scan/SCANCWAThreatIndex" />
    </contribute>
    <contribute xsi:type="subMenu" menuText="Hail Diagnostic Grids from ${icao}"
        id="HailDiagnosticGrids${icao}">
