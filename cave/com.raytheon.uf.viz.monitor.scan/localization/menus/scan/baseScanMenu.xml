            menuText="QPF .10" id="${icao}QPF .10">
            <substitute key="icao" value="${icao}" />
            <substitute key="fieldName" value="P10" />
        </contribute>
        <contribute xsi:type="bundleItem" file="bundles/scan/QPF.xml"
            menuText="QPF .25" id="${icao}QPF .25">
