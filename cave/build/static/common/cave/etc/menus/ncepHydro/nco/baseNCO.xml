               menuText="Daily Precipitation" id="dailyPrecipitation">
	        <dataURI>/redbook/%/PEWA31/%</dataURI>
	        <substitute key="wmo" value="PEWA31"/>
	    </contribute>
	    <contribute xsi:type="bundleItem" file="bundles/Redbook.xml" 
