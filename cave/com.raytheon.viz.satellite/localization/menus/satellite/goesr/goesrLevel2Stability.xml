    <!-- TOWRdocs Header
         GOES-R Level2 Stability menu file.
    -->
    <!-- TOWRdocs Description
         This file modifies color maps for the stability indices where commented/indicated.
     Note that the cmap color-cape-10 is used in several indices purposefully, and not just CAPE.
    -->
    <!-- TOWRdocs Status
         Modification of previous baseline menu file of the same name.
     3/28/2017 adds GOES-R sub dir, removed Sat subdir.
    -->
    <!-- TOWRdocs POC
         Lee Byerle 3/28/2017
   -->
<menuTemplate xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <contribute xsi:type="bundleItem" file="bundles/DefaultSatellite.xml" 
                menuText="CAPE" id="goesr-cape">
        <substitute key="entity" value="GOES%" />
        <substitute key="element" value="CAPE"/>
        <substitute key="colormap" value="GOES-R/GOESR-L2/color-cape-10"/>
    </contribute>
    <contribute xsi:type="bundleItem" file="bundles/DefaultSatellite.xml" 
                menuText="K-Index" id="goesr-ki">
        <substitute key="entity" value="GOES%" />
        <substitute key="element" value="KI"/>
        <substitute key="colormap" value="GOES-R/GOESR-L2/color-cape-10"/>
    </contribute>
    <contribute xsi:type="bundleItem" file="bundles/DefaultSatellite.xml" 
                menuText="Lifted Index" id="goer-li">
        <substitute key="entity" value="GOES%" />
        <substitute key="element" value="LI"/>
        <substitute key="colormap" value="GOES-R/GOESR-L2/color-li-10"/>
    </contribute>
    <contribute xsi:type="bundleItem" file="bundles/DefaultSatellite.xml" 
                menuText="Showalter Index" id="goer-si">
        <substitute key="entity" value="GOES%" />
        <substitute key="element" value="SI"/>
        <substitute key="colormap" value="GOES-R/GOESR-L2/color-cape-10"/>
    </contribute>
    <contribute xsi:type="bundleItem" file="bundles/DefaultSatellite.xml" 
                menuText="Total Totals" id="goer-tt">
        <substitute key="entity" value="GOES%" />
        <substitute key="element" value="TT"/>
        <substitute key="colormap" value="GOES-R/GOESR-L2/color-cape-10"/>
    </contribute>
</menuTemplate>
