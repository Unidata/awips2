<plotParameterDefinitions>
<plugin>bufrssmi</plugin>

<plotParameterDefinition displayName="validTime" displayType="TEXT" format="time" param="validTime" sampleValue="T" svgClass="text" symbol="Z" />

<plotParameterDefinition displayName="satelliteID" displayType="TEXT" lookupTable="sat_name_lookup.txt" param="satelliteID" sampleValue="0" svgClass="text" />

<plotParameterDefinition displayName="orbitNumber" displayType="TEXT" format="%8.0f" param="orbitNumber" sampleValue="0" svgClass="text" />

<plotParameterDefinition displayName="scanNumber" displayType="TEXT" format="%5.0f" param="scanNumber" sampleValue="0" svgClass="text" />

<plotParameterDefinition displayName="latitude" displayType="TEXT" format="%3.2f" param="latitude" sampleValue="0" svgClass="text" symbol="deg" />

<plotParameterDefinition displayName="longitude" displayType="TEXT" format="%3.2f" param="longitude" sampleValue="0" svgClass="text" symbol="deg" />

<plotParameterDefinition displayName="windSpeed (kts)" displayType="RANGE" format="%2.1f" lookupTable="output_formats.txt" param="windSpeed" sampleValue="59" svgClass="text" unit="kts" />

<plotParameterDefinition displayName="Marker" displayType="MARKER" param="Marker" sampleValue=" " svgClass="marker" />

<plotParameterDefinition displayName="precipWater-1" displayType="TEXT" format="%2.1f" lookupTable="output_formats.txt" param="precipWater" sampleValue="0" svgClass="text" symbol="kg/(m**2)" />

<plotParameterDefinition displayName="precipWater" displayType="RANGE" format="%2.1f" lookupTable="output_formats.txt" param="precipWater" sampleValue="0" svgClass="text" />

<plotParameterDefinition displayName="seaTemp (°C)" displayType="TEXT" format="%2.1f" param="seaTemp" sampleValue="0" svgClass="text" symbol="C" unit="°C" />

<plotParameterDefinition displayName="vertIntegWater-1" displayType="TEXT" format="%2.1f" lookupTable="output_formats.txt" param="vertIntegWater" sampleValue="75" svgClass="text" symbol="kg/(m**2)" />

<plotParameterDefinition displayName="vertIntegWater" displayType="RANGE" format="%2.2f" lookupTable="output_formats.txt" param="vertIntegWater" sampleValue="0" svgClass="text" />
</plotParameterDefinitions>