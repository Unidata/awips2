<?eclipse version="3.3"?>
<plugin>
   <extension
         id="CAVE"
         name="Raytheon Viz"
         point="org.eclipse.core.runtime.products">
      <product
            application="com.raytheon.uf.viz.application.application"
            name="CAVE">
         <property
               name="aboutText"
               value="%caveAboutText"/>
         <property
               name="aboutImage"
               value="icons/ipr.gif"/>
         <property
               name="appName"
               value="CAVE">
         </property>
         <property
               name="startupProgressRect"
               value="3,234,413,13">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         <property
               name="startupMessageRect"
               value="7,252,445,20">
         </property>
         <property
               name="startupForegroundColor"
               value="000000">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.statusHandlers">
      <statusHandlerProductBinding
            handlerId="com.raytheon.viz.notification.statusHandler"
            productId="com.raytheon.viz.product.awips.CAVE">
      </statusHandlerProductBinding>
   </extension>
   <extension
         id="com.raytheon.viz.ui.bindings"
         point="org.eclipse.ui.bindings">
      <!-- These keys represent default bindings we want to override --> 
      <key
            commandId="com.raytheon.viz.ui.actions.nullAction2"
            schemeId="com.raytheon.viz.ui.awips.scheme"
            sequence="M1+M2+T">
      </key>
      <key
            commandId="com.raytheon.viz.ui.actions.nullAction2"
            schemeId="com.raytheon.viz.ui.awips.scheme"
            sequence="M1+M2+R">
      </key>
   </extension>
</plugin>
