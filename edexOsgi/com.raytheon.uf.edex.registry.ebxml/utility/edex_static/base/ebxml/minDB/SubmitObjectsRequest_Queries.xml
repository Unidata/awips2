                    <rim:SlotValue xsi:type="rim:StringValueType">
                        <rim:Value>urn:oasis:names:tc:ebxml-regrep:classificationScheme:ObjectType</rim:Value>
                    </rim:SlotValue>
                </rim:Slot>
                <rim:Name>
