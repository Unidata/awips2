                    <rim:LocalizedString value="Query Language"/>
                </rim:Name>
                <rim:Description>
                    <rim:LocalizedString value="Value is the id of a ClassificationNode within the canonical QueryLanguageScheme ClassificationScheme."/>
                </rim:Description>
            </rim:Parameter>
