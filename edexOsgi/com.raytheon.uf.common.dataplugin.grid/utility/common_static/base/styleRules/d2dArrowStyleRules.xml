<styleRuleset>

	<styleRule>
		<paramLevelMatches>
			<parameter>qVec</parameter>
			<parameter>qnVec</parameter>
			<parameter>qsVec</parameter>
			<parameter>Q Vectors</parameter>
			<parameter>Qn Vectors</parameter>
			<parameter>Qs Vectors</parameter>
		</paramLevelMatches>
		<arrowStyle>
			<displayUnits >K/m^2/s*1.0E-12</displayUnits>
			<scale>-2.5</scale>
		</arrowStyle>
	</styleRule>

	<styleRule>
		<paramLevelMatches>
			<parameter>FVecs</parameter>
			<parameter>FsVecs</parameter>
			<parameter>FnVecs</parameter>
			<parameter>Frontogenesis Vectors</parameter>
			<parameter>Fn Vectors</parameter>
			<parameter>Fs Vectors</parameter>
		</paramLevelMatches>
		<arrowStyle>
			<displayUnits label="K/m/1e10s">K/m/s*1.0E-10</displayUnits>
			<scale>-1.25</scale>
		</arrowStyle>
	</styleRule>

	<styleRule>
		<paramLevelMatches>
			<parameter>BlkShr</parameter>
			<parameter>geoW</parameter>
			<parameter>ageoW</parameter>
			<parameter>Geostrophic Wind</parameter>
			<parameter>Ageo Wind</parameter>
			<parameter>SNW</parameter>
		</paramLevelMatches>
		<arrowStyle>
			<displayUnits>kts</displayUnits>
		</arrowStyle>
	</styleRule>
	
	<styleRule>
		<paramLevelMatches>
			<parameter>Shear</parameter>
		</paramLevelMatches>
		<arrowStyle>
			<displayUnits>kn/km</displayUnits>
		</arrowStyle>
	</styleRule>

	<styleRule>
		<paramLevelMatches>
			<parameter>TGrd</parameter>
		</paramLevelMatches>
		<arrowStyle>
		<!-- 
			<displayUnits>K/(km * 1000)</displayUnits>
		<displayUnits label="K/(km*1000)">K/m*1.0E6</displayUnits>
		-->		
		<displayUnits label="C/m">K/m*1.0E6</displayUnits>
		</arrowStyle>
	</styleRule>

	<styleRule>
		<paramLevelMatches>
			<parameter>EPTGrd</parameter>
		</paramLevelMatches>
		<arrowStyle>
			<displayUnits>K/(km * 100)</displayUnits>
		</arrowStyle>
	</styleRule>

	<styleRule>
		<paramLevelMatches>
			<parameter>defV</parameter>
		</paramLevelMatches>
		<arrowStyle>
			<displayUnits>(sec^-1)*1.0E-5</displayUnits>
			<scale>2.5</scale>
		</arrowStyle>
	</styleRule>

	<styleRule>
		<paramLevelMatches>
			<parameter>Wind</parameter>
			<parameter>Gust</parameter>
		</paramLevelMatches>
		<arrowStyle>
			<displayUnits>kts</displayUnits>
		</arrowStyle>
	</styleRule>
	
	<styleRule>
		<paramLevelMatches>
			<parameter>Windmean</parameter>
		</paramLevelMatches>
		<arrowStyle>
			<displayUnits>kts</displayUnits>
		</arrowStyle>
	</styleRule>
	
	<styleRule>
		<paramLevelMatches>
			<parameter>MTV</parameter>
		</paramLevelMatches>
		<arrowStyle>
			<displayUnits label="gm/kgs">g*m/(kg*s)</displayUnits>
		</arrowStyle>
	</styleRule>
	<styleRule>
		<paramLevelMatches>
			<parameter>OGRD</parameter>
			<parameter>BARO</parameter>
		</paramLevelMatches>
		<arrowStyle>
			<displayUnits>m/min</displayUnits>
		</arrowStyle>
	</styleRule>
