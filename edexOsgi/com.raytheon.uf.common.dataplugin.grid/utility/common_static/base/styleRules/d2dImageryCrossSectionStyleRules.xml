<styleRuleset>
	<styleRule>
		<paramLevelMatches>
			<parameter>Into</parameter>
			<parameter>Along</parameter>
			<parameter>IntoA</parameter>
			<parameter>AlongA</parameter>
		</paramLevelMatches>
		<imageStyle>
			<displayUnits>kts</displayUnits>
			<defaultColormap>Grid/gridded data</defaultColormap>
		</imageStyle>
	</styleRule>
	<styleRule>
		<paramLevelMatches>
			<parameter>wSp</parameter>
			<parameter>wSp_avg</parameter>
		</paramLevelMatches>
		<imageStyle>
			<displayUnits>kts</displayUnits>
			<range scale="LINEAR">
				<minValue>0</minValue>
				<maxValue>200</maxValue>
			</range>
			<defaultColormap>Grid/gridded data</defaultColormap>
			<colorbarLabeling>
				<increment>20</increment>
			</colorbarLabeling>
		</imageStyle>
	</styleRule>
	<styleRule>
		<paramLevelMatches>
			<parameter>RRV</parameter>
		</paramLevelMatches>
		<imageStyle>
			<displayUnits>kts</displayUnits>
			<range scale="LINEAR">
				<minValue>-128</minValue>
				<maxValue>125</maxValue>
			</range>
			<defaultColormap>Radar/GSD/8 bit Vel</defaultColormap>
			<colorbarLabeling>
				<increment>20</increment>
			</colorbarLabeling>
		</imageStyle>
	</styleRule>
	<styleRule>
		<paramLevelMatches>
			<parameter>RR</parameter>
			<parameter>CXR</parameter>
			<parameter>LLR</parameter>
		</paramLevelMatches>
		<imageStyle>
			<displayUnits>dBZ</displayUnits>
			<range scale="LINEAR">
				<minValue>0</minValue>
				<maxValue>80</maxValue>
			</range>
			<defaultColormap>Radar/Storm Total Precip</defaultColormap>
			<colorbarLabeling>
				<increment>5</increment>
			</colorbarLabeling>
		</imageStyle>
	</styleRule>
</styleRuleset>
