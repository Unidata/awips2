<!--
    Clouds, icing, turbulence description data
-->
<pointDataDescription>
  <dimension name="maxEntries" length="100"/>
  <!-- Time in seconds. To use in Java - multiply by 1000 -->
  <parameter name="validTime" numDims="1" type="LONG" />

  <parameter name="baseHgt" numDims="1" type="FLOAT" unit="m"/>
  <parameter name="topHgt" numDims="1" type="FLOAT" unit="m"/>

  <parameter name="featureType" numDims="1" type="INT" />
  <parameter name="dimensionSig" numDims="1" type="INT" />
  <parameter name="cloudDistribution" numDims="1" type="INT" />
  <parameter name="cloudType" numDims="1" type="INT" />
  <parameter name="cloudBase" numDims="1" type="FLOAT" unit="m"/>
  <parameter name="cloudTop" numDims="1" type="FLOAT" unit="m"/>

  <parameter name="numOfPoints" numDims="1" type="INT" />
  <parameter name="latitude" numDims="2" type="FLOAT" unit="degree_N" dimension="maxEntries"/>
  <parameter name="longitude" numDims="2" type="FLOAT" unit="degree_E" dimension="maxEntries"/>
</pointDataDescription>
