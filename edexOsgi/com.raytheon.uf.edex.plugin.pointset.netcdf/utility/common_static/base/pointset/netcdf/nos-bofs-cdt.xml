<pointSetProductDescriptions>

  <default>
    <matches>
      <match pattern="^nos\.[cdt]bofs\.fields\.[nf].*">
        <filename />
      </match>
    </matches>

    <triangulation type="delauney" />

    <level>
      <masterLevel>
        <value value="SCOORD" />
      </masterLevel>
      <levelOneValue>
        <variable name="s_rho" />
      </levelOneValue>
    </level>
  </default>

  <!-- Temperature -->
  <description>
    <data>
        <variable name="temp" />
        <discreteMask name="mask_rho" value="0" keep="false">
          <fillFloat>NaN</fillFloat>
        </discreteMask>
    </data>

    <parameter>
        <abbreviation>
            <value value="WTMP" />
        </abbreviation>
        <units>
            <value value="c" />
        </units>
    </parameter>
  </description>

  <!-- Salinity -->
  <description>
    <data>
        <variable name="salt" />
        <discreteMask name="mask_rho" value="0" keep="false">
          <fillFloat>NaN</fillFloat>
        </discreteMask>
    </data>

    <parameter>
        <abbreviation>
            <value value="SALIN" />
        </abbreviation>
        <units>
            <value value="PSU" />
        </units>
    </parameter>
  </description>

  <!-- Current speed in u-direction -->
  <description>
    <data>
        <variable name="u" />
        <discreteMask name="mask_u" value="0" keep="false">
          <fillFloat>NaN</fillFloat>
        </discreteMask>
    </data>

    <parameter>
        <abbreviation>
            <value value="UOGRD" />
        </abbreviation>
        <units>
            <value value="m/s" />
        </units>
    </parameter>
  </description>

  <!-- Current speed in v-direction -->
  <description>
    <data>
        <variable name="v" />
        <discreteMask name="mask_v" value="0" keep="false">
          <fillFloat>NaN</fillFloat>
        </discreteMask>
    </data>

    <parameter>
        <abbreviation>
            <value value="VOGRD" />
        </abbreviation>
        <units>
            <value value="m/s" />
        </units>
    </parameter>
  </description>

  <!-- Free-Surface -->
  <description>
    <data>
        <variable name="zeta" />
        <discreteMask name="mask_rho" value="0" keep="false">
          <fillFloat>NaN</fillFloat>
        </discreteMask>
    </data>

    <parameter>
        <abbreviation>
            <value value="DSLM" />
        </abbreviation>
        <units>
            <value value="m" />
        </units>
    </parameter>

    <level>
      <masterLevel>
        <value value="SFC" />
      </masterLevel>
      <levelOneValue>
        <value value="0.0" />
      </levelOneValue>
    </level>
  </description>

  <!-- FORECAST-specific fields  -->
  <description>
    <matches>
      <match pattern="^nos\.[cdt]bofs\.fields\.f.*">
        <filename />
      </match>
    </matches>

    <dataTime>
      <formattedRefTime dateFormat="yyyyMMdd HH'Z'">
        <pattern inputPattern="^nos\.[cdt]bofs\.fields\.[nf]\d{3}\.(\d{8})\.t(\d{2}z).*" outputFormat="%S %S">
          <filename />
        </pattern>
      </formattedRefTime>
      <epochOffsetValidTime epoch="2009-01-01 00:00:00" units="SECONDS">
        <variable name="ocean_time" />
      </epochOffsetValidTime>
    </dataTime>

    <datasetId>
      <pattern inputPattern="^([a-zA-Z]+).*" outputFormat="NOS-%S-FORECAST">
        <attribute name="title" />
      </pattern>
    </datasetId>
  </description>


  <!-- NOWCAST-specific fields  -->
  <description allowRecordOverwrite="true">
    <matches>
      <match pattern="^nos\.[cdt]bofs\.fields\.n.*">
        <filename />
      </match>
    </matches>

    <dataTime>
      <epochOffsetRefTime epoch="2009-01-01 00:00:00" units="SECONDS">
        <variable name="ocean_time" />
      </epochOffsetRefTime>
    </dataTime>

    <datasetId>
      <pattern inputPattern="^([a-zA-Z]+).*" outputFormat="NOS-%S-NOWCAST">
        <attribute name="title" />
      </pattern>
    </datasetId>
  </description>
</pointSetProductDescriptions>