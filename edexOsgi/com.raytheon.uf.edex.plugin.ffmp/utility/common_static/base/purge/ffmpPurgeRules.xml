    <!--
        Purge file for FFMP
        period tag is where you change the time:
            ex. 01-00:00:00 is one day or 24 hours of retention
            ex. 00-12:00:00 is 12 hours

        SourceName key is required in order for ARCHIVE types to NOT not be purged.
    -->

    <!--
        This is an absolute override file, indicating that a higher priority
        version of the file will completely replace a lower priority version
        of the file.

        Incremental overrides can also be achieved by creating a new file with
        a suffix inserted between the original filename and extension.
    -->
<purgeRuleSet>
    <key>sourceName</key>
    <defaultRule>
        <!-- keep 24 hours for most data -->
        <period>00-24:00:00</period>
    </defaultRule>
    <rule regex="true">
        <!-- This rule keeps 1 version of each ARI data regardless of its
             reference time. If an update is ever loaded, the older version
             will be purged. -->
        <keyValue>ARI.*</keyValue>
        <versionsToKeep>1</versionsToKeep>
    </rule>
    <rule regex="true">
        <!-- DR 13325 keep 24 versions of FFG -->
        <keyValue>FFG.*</keyValue>
        <versionsToKeep>24</versionsToKeep>
    </rule>
</purgeRuleSet>