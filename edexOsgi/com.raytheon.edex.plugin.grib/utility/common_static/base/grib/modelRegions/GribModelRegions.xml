    <!--
        This is an incremental override file, indicating that the files at
        different localization levels will be combined. Since all the files in
        the directory are read, for most custom additions a unique file should
        be added to the directory instead of overriding a specific file.
    -->
<gribRegionSet>

   <!--  List of known supported regions and their bounds. Currently does not support regions that cross the date line. -->

   <!-- Alaska (Note that since this is a polar stereographic projection, this region is a simple lat/lon approximation) -->
   <region>
      <name>AK</name>
      <lat1>85.0</lat1>
      <lon1>-180.0</lon1>
      <lat2>35.0</lat2>
      <lon2>-120.0</lon2>
   </region>

   <!-- United States (CONUS) -->
   <region>
      <name>US</name>
      <lat1>54.6</lat1>
      <lon1>-153.0</lon1>
      <lat2>14.2</lat2>
      <lon2>-65.0</lon2>
   </region>
   <!--  Hawaii -->
