        name="ice"
        type="java.lang.String"
        column="ice"
        length="160"
    />
    <property
        name="reach"
        type="java.lang.String"
        column="reach"
        length="80"
    />
    <property
        name="res"
        type="java.lang.String"
        column="res"
        length="255"
    />
    <property
        name="topo"
        type="java.lang.String"
        column="topo"
        length="255"
    />

    <!-- Associations -->
  
    <!-- bi-directional one-to-one association to Riverstat -->
    <one-to-one
        name="riverstat"
        class="com.raytheon.edex.db.objects.ihfs.Riverstat"
        outer-join="auto"
        constrained="true"
    />
    <!-- bi-directional many-to-one association to Proximity -->
    <many-to-one
        name="proximity"
        class="com.raytheon.edex.db.objects.ihfs.Proximity"
        not-null="true"
    >
        <column name="proximity" />
    </many-to-one>

</class>
