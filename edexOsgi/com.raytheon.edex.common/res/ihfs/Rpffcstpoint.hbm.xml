<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping default-lazy="true">
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.raytheon.edex.db.objects.ihfs.Rpffcstpoint" 
    table="rpffcstpoint"
>

    <id
        name="lid"
        type="java.lang.String"
        column="lid"
    >
        <generator class="assigned" />
    </id>

    <property
        name="ordinal"
        type="java.lang.Integer"
        column="ordinal"
        length="10"
    />
    <property
        name="chgThreshold"
        type="java.lang.Double"
        column="chg_threshold"
        length="17"
    />
    <property
        name="recType"
        type="java.lang.String"
        column="rec_type"
        length="3"
    />
    <property
        name="secondaryBack"
        type="java.lang.String"
        column="secondary_back"
        not-null="true"
        length="3"
    />
    <property
        name="backhrs"
        type="java.lang.Integer"
        column="backhrs"
        length="10"
    />
    <property
        name="forwardhrs"
        type="java.lang.Integer"
        column="forwardhrs"
        length="10"
    />
    <property
        name="adjustendhrs"
        type="java.lang.Double"
        column="adjustendhrs"
        length="17"
    />

    <!-- Associations -->
  
    <!-- bi-directional one-to-one association to Riverstat -->
    <one-to-one
        name="riverstat"
        class="com.raytheon.edex.db.objects.ihfs.Riverstat"
        outer-join="auto"
        constrained="true"
    />
    <!-- bi-directional many-to-one association to Rpffcstgroup -->
    <many-to-one
        name="rpffcstgroup"
        class="com.raytheon.edex.db.objects.ihfs.Rpffcstgroup"
        not-null="true"
    >
        <column name="group_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to Hsa -->
    <many-to-one
        name="hsa"
        class="com.raytheon.edex.db.objects.ihfs.Hsa"
        not-null="true"
    >
        <column name="primary_back" />
    </many-to-one>

</class>
</hibernate-mapping>
