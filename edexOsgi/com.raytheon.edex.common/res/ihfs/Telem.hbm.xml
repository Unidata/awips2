<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping default-lazy="true">
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.raytheon.edex.db.objects.ihfs.Telem" 
    table="telem"
>

    <id
        name="lid"
        type="java.lang.String"
        column="lid"
    >
        <generator class="assigned" />
    </id>

    <property
        name="cost"
        type="java.lang.Double"
        column="cost"
        length="17"
    />
    <property
        name="criteria"
        type="java.lang.String"
        column="criteria"
        length="50"
    />
    <property
        name="phone"
        type="java.lang.String"
        column="phone"
        length="12"
    />
    <property
        name="sensorid"
        type="java.lang.String"
        column="sensorid"
        length="10"
    />
    <property
        name="rptfreq"
        type="java.lang.String"
        column="rptfreq"
        length="4"
    />
    <property
        name="notify"
        type="java.lang.String"
        column="notify"
        length="1"
    />

    <!-- Associations -->
  
    <!-- bi-directional one-to-one association to Location -->
    <one-to-one
        name="location"
        class="com.raytheon.edex.db.objects.ihfs.Location"
        outer-join="auto"
        constrained="true"
    />
    <!-- bi-directional many-to-one association to Telmtype -->
    <many-to-one
        name="telmtype"
        class="com.raytheon.edex.db.objects.ihfs.Telmtype"
        not-null="true"
    >
        <column name="type" />
    </many-to-one>
    <!-- bi-directional many-to-one association to Telmowner -->
    <many-to-one
        name="telmowner"
        class="com.raytheon.edex.db.objects.ihfs.Telmowner"
        not-null="true"
    >
        <column name="owner" />
    </many-to-one>
    <!-- bi-directional many-to-one association to Telmpayor -->
    <many-to-one
        name="telmpayor"
        class="com.raytheon.edex.db.objects.ihfs.Telmpayor"
        not-null="true"
    >
        <column name="payor" />
    </many-to-one>

</class>
</hibernate-mapping>
