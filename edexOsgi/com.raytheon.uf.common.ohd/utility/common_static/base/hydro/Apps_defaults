                                        # when errors occur (=IF_ERROR)
adb_shef_raw_post_unk  : IDS_AND_DATA   # NONE - do not post to the UnkStnValue tables
                         		        # values IDS_ONLY or IDS_AND_DATA
                          		        # will post everything
                          		        # to the UnkStnValue table
