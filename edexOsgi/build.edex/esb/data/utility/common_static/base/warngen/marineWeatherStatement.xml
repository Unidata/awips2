 <bullets>
  <bullet bulletText="  *****BASIS SECTION*****...(Choose 1) " bulletType="title"/>
  <bullet bulletName="doppler" bulletText="Doppler radar indicated" bulletGroup="group5" bulletDefault="true" parseString="DOPPLER RADAR INDICATED"/>
