  <bullet bulletName="pilot" bulletText="Pilot reported" bulletGroup="group5" parseString="A PILOT REPORTED"/>
  <bullet bulletText="" bulletType="title"/>
  <bullet bulletText="  ****EVENT TYPE SECTION****...(Choose 1) " bulletType="title"/>
