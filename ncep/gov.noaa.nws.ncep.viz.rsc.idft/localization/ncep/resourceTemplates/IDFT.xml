<resourceGroup xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <resource>
          <loadProperties loadWithoutData="true"  overlayMatchTolerance="1" >
           <!-- capabilities>
              <capability xsi:type="imagingCapability" interpolationState="false" brightness="1.0" contrast="1.0" alpha="1.0"/>
              </capabilities -->
              <resourceType>PLAN_VIEW</resourceType >
            </loadProperties>
          <properties isSystemResource="false" isBlinking="false" isMapLayer="false" isHoverOn="false" isVisible="true">
            </properties>
          <resourceData xsi:type="NC-IDFTResourceData" isUpdatingOnMetadataOnly="false" isRequeryNecessaryOnTimeMatch="false">
           <timeMatchMethod>${timeMatchMethod}</timeMatchMethod>
           <timelineGenMethod>${timelineGenMethod}</timelineGenMethod> 
           <frameSpan>${frameSpan}</frameSpan>
           <dfltNumFrames>${dfltNumFrames}</dfltNumFrames>
           <dfltTimeRange>${dfltTimeRange}</dfltTimeRange>
           <!-- <legendColor>$ {legend_color}</legendColor> -->
           <distanceColor>${distanceColor}</distanceColor>
           <symbolName>${symbolName}</symbolName>
           <pointSize>${pointSize}</pointSize>
           <pointColor>${pointColor}</pointColor>
           <arrowColor>${arrowColor}</arrowColor>
           <arrowLength>${arrowLength}</arrowLength>
           <arrowLineWidth>${arrowLineWidth}</arrowLineWidth>
           <requestStyle>METADATA</requestStyle>  
           <metadataMap>
				<mapping key="pluginName">
					<constraint constraintValue="${pluginName}" constraintType="EQUALS" />
				</mapping>
				<mapping key="reportType">
					<constraint constraintValue="${reportType}" constraintType="EQUALS" />
				</mapping>
            </metadataMap>
            
           
          </resourceData>
        </resource>
</resourceGroup>
