            paramType="EDITABLE_ATTRIBUTE"
            ncResourceName="DbOverlay">
      </nc-resourceParameter>
      <nc-resourceParameter
            paramClass="Integer"
            paramName="lineWidth"
            defaultValue="1"
            paramType="EDITABLE_ATTRIBUTE"
            ncResourceName="DbOverlay">
      </nc-resourceParameter>
      <nc-resourceParameter
            paramClass="String"
            paramName="dbName"
            paramType="IMPLEMENTATION_PARAM"
            ncResourceName="DbOverlay">
      </nc-resourceParameter>
      <nc-resourceParameter
            paramClass="String"
            paramName="tableName"
            paramType="IMPLEMENTATION_PARAM"
            ncResourceName="DbOverlay">
      </nc-resourceParameter>
      <nc-resourceParameter
            paramClass="String"
            paramName="labelField"
