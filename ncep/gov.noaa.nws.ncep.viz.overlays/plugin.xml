   <extension
          point="com.raytheon.uf.viz.localization.localizationpath">
       <path
             application="NCEP"
             localizationType="CAVE_STATIC"
             name="basemaps"
             recursive="false"
             value="ncep/basemaps"
	         extensionFilter=".lpi,.spi">
       </path>
       <path
             application="NCEP"
             localizationType="CAVE_STATIC"
             name="StaticPgenOverlays"
             recursive="false"
             value="ncep/StaticPgenOverlays"
	         extensionFilter=".xml">
       </path>
    </extension>
   
