      </nc-resourceParameter>
      <nc-resourceParameter
            paramClass="String"
            paramName="mapName"
            paramType="IMPLEMENTATION_PARAM"
            ncResourceName="DbOverlay">
      </nc-resourceParameter>

<!-- The Resource for displaying static PGEN Products -->
