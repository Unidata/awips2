<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<resourceGroup xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
   <resource>
      <loadProperties loadWithoutData="true"
         overlayMatchTolerance="1">
         <resourceType>PLAN_VIEW</resourceType>
      </loadProperties>
      <properties isSystemResource="false" isBlinking="false"
         isMapLayer="false" isHoverOn="false" isVisible="true">
      </properties>
      <resourceData xsi:type="LightningResourceData"
            isUpdatingOnMetadataOnly="false" isRequeryNecessaryOnTimeMatch="false">
		 <legendColor>${legendColor}</legendColor>						
         <colorByIntensity>${colorByIntensity}</colorByIntensity>
         <timeMatchMethod>${timeMatchMethod}</timeMatchMethod>       
         <timelineGenMethod>${timelineGenMethod}</timelineGenMethod> 
         <frameSpan>${frameSpan}</frameSpan>
         <dfltNumFrames>${dfltNumFrames}</dfltNumFrames>
         <dfltTimeRange>${dfltTimeRange}</dfltTimeRange>
         <enablePositiveStrikes>${enablePositiveStrikes}</enablePositiveStrikes>
         <enableNegativeStrikes>${enableNegativeStrikes}</enableNegativeStrikes>
         <positiveSymbolSize>${positiveSymbolSize}</positiveSymbolSize>
         <negativeSymbolSize>${negativeSymbolSize}</negativeSymbolSize>
         <requestStyle>METADATA</requestStyle>
         ${colorBar}
         <metadataMap>
            <mapping key="pluginName">
				<constraint constraintValue="${pluginName}" constraintType="EQUALS" />
            </mapping>
         </metadataMap>
      </resourceData>
   </resource>
</resourceGroup>
