<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<resourceGroup xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <resource>
          <loadProperties loadWithoutData="true"></loadProperties>
          <properties isSystemResource="false"
              isBlinking="false" isMapLayer="false" isHoverOn="false"
              isVisible="true" />
          <resourceData xsi:type="NC-WCPResourceData"
                        isUpdatingOnMetadataOnly="false" 
                        isRequeryNecessaryOnTimeMatch="false"> 
            <frameSpan>${frameSpan}</frameSpan>
            <dfltNumFrames>${dfltNumFrames}</dfltNumFrames>
            <dfltTimeRange>${dfltTimeRange}</dfltTimeRange>
            <timeMatchMethod>${timeMatchMethod}</timeMatchMethod>       
            <timelineGenMethod>${timelineGenMethod}</timelineGenMethod> 
            <sourceName>WCP</sourceName>
		    <legendColor>${color}</legendColor>
		    <color>${color}</color>
			<lineWidth>${lineWidth}</lineWidth>
		
			<thunderstormName>${thunderstormName}
			</thunderstormName>
			<thunderstormEnable>${thunderstormEnable}
			</thunderstormEnable>
			<thunderstormColor>${thunderstormColor}
			</thunderstormColor>
		
			<tornadoName>${tornadoName}</tornadoName>
			<tornadoEnable>${tornadoEnable}</tornadoEnable>
			<tornadoColor>${tornadoColor}</tornadoColor>
		
			<watch0Name>${watch0Name}</watch0Name>
			<watch0Enable>${watch0Enable}</watch0Enable>
			<watch0Color>${watch0Color}</watch0Color>
			<watch1Name>${watch1Name}</watch1Name>
			<watch1Enable>${watch1Enable}</watch1Enable>
			<watch1Color>${watch1Color}</watch1Color>
			<watch2Name>${watch2Name}</watch2Name>
			<watch2Enable>${watch2Enable}</watch2Enable>
			<watch2Color>${watch2Color}</watch2Color>
			<watch3Name>${watch3Name}</watch3Name>
			<watch3Enable>${watch3Enable}</watch3Enable>
			<watch3Color>${watch3Color}</watch3Color>
			<watch4Name>${watch4Name}</watch4Name>
			<watch4Enable>${watch4Enable}</watch4Enable>
			<watch4Color>${watch4Color}</watch4Color>
			<watch5Name>${watch5Name}</watch5Name>
			<watch5Enable>${watch5Enable}</watch5Enable>
			<watch5Color>${watch5Color}</watch5Color>
			<watch6Name>${watch6Name}</watch6Name>
			<watch6Enable>${watch6Enable}</watch6Enable>
			<watch6Color>${watch6Color}</watch6Color>
			<watch7Name>${watch7Name}</watch7Name>
			<watch7Enable>${watch7Enable}</watch7Enable>
			<watch7Color>${watch7Color}</watch7Color>
			<watch8Name>${watch8Name}</watch8Name>
			<watch8Enable>${watch8Enable}</watch8Enable>
			<watch8Color>${watch8Color}</watch8Color>
			<watch9Name>${watch9Name}</watch9Name>
			<watch9Enable>${watch9Enable}</watch9Enable>
			<watch9Color>${watch9Color}</watch9Color>
		
			<timeLabelName>${timeLabelName}</timeLabelName>
			<timeLabelEnable>${timeLabelEnable}</timeLabelEnable>
			<watchNumberLabelName>${watchNumberLabelName}
			</watchNumberLabelName>
			<watchNumberLabelEnable>${watchNumberLabelEnable}
			</watchNumberLabelEnable>
			<colorCodeName>${colorCodeName}</colorCodeName>
			<colorCodeEnable>${colorCodeEnable}</colorCodeEnable>
		
			<requestStyle>METADATA</requestStyle>
             <metadataMap>
               <mapping key="pluginName">
                 <constraint constraintValue="${pluginName}" constraintType="EQUALS" />
               </mapping>
               <mapping key="reportType">
                 <constraint constraintValue="${reportType}" constraintType="EQUALS" />
               </mapping>
             </metadataMap>
          </resourceData>
        </resource>
</resourceGroup>        
