<!-- STATION VARIABLES 
	STNM, SLAT, SLON, 
	-->
 <PlotParameterDefn  plotParamName="STID" metParamName="StationID"  
                     dbParamName="stationName" />
 <PlotParameterDefn  plotParamName="SLAT" metParamName="StationLatitude"  
                     dbParamName="latitude" plotUnit="°"/>
