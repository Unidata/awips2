<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<resourceGroup xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <resource>
          <loadProperties loadWithoutData="true"></loadProperties>
          <properties isSystemResource="false"
              isBlinking="false" isMapLayer="false" isHoverOn="false"
              isVisible="true" />
          <resourceData xsi:type="NC-ConvSigmetResourceData"
                        isUpdatingOnMetadataOnly="false" 
                        isRequeryNecessaryOnTimeMatch="false"> 
             <frameSpan>${frameSpan}</frameSpan>
             <dfltNumFrames>${dfltNumFrames}</dfltNumFrames>                          
             <dfltTimeRange>${dfltTimeRange}</dfltTimeRange>
             <timeMatchMethod>${timeMatchMethod}</timeMatchMethod>       
             <timelineGenMethod>${timelineGenMethod}</timelineGenMethod> 
             <sourceName>${legendName}</sourceName>
	     	 <legendColor>${color}</legendColor>						
             <color>${color}</color>
             <hour0Enable>${hour0Enable}</hour0Enable>
             <hour0Color>${hour0Color}</hour0Color>
             <hour0LineWidth>${hour0LineWidth}</hour0LineWidth>
             <hour1Enable>${hour1Enable}</hour1Enable>
             <hour1Color>${hour1Color}</hour1Color>
             <hour1LineWidth>${hour1LineWidth}</hour1LineWidth>
             <hour2Enable>${hour2Enable}</hour2Enable>
             <hour2Color>${hour2Color}</hour2Color>
             <hour2LineWidth>${hour2LineWidth}</hour2LineWidth>
             <outlookEnable>${outlookEnable}</outlookEnable>
             <outlookColor>${outlookColor}</outlookColor>
             <outlookLineWidth>${outlookLineWidth}</outlookLineWidth>
             <hour0sequenceIdEnable>${hour0sequenceIdEnable}</hour0sequenceIdEnable>
             <hour1sequenceIdEnable>${hour1sequenceIdEnable}</hour1sequenceIdEnable>
             <hour2sequenceIdEnable>${hour2sequenceIdEnable}</hour2sequenceIdEnable>
             <timeEnable>${timeEnable}</timeEnable>
             <motionEnable>${motionEnable}</motionEnable>
             <flightLevelEnable>${flightLevelEnable}</flightLevelEnable>
             <intensityEnable>${intensityEnable}</intensityEnable>
             <requestStyle>METADATA</requestStyle>
             <metadataMap>
               <mapping key="pluginName">
                 <constraint constraintValue="${pluginName}" constraintType="EQUALS" />
               </mapping>
               <mapping key="reportType">
                 <constraint constraintValue="${reportType}" constraintType="EQUALS" />
               </mapping>
             </metadataMap>             
          </resourceData>
        </resource>
</resourceGroup>        
